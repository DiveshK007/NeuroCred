{
  "monitors": [
    {
      "friendly_name": "NeuroCred API - Liveness",
      "url": "https://api.neurocred.io/health",
      "type": 1,
      "interval": 300,
      "timeout": 30,
      "keyword_type": 1,
      "keyword_value": "\"status\":\"healthy\"",
      "alert_contacts": []
    },
    {
      "friendly_name": "NeuroCred API - Readiness",
      "url": "https://api.neurocred.io/health/ready",
      "type": 1,
      "interval": 300,
      "timeout": 30,
      "keyword_type": 1,
      "keyword_value": "\"status\":\"ready\"",
      "alert_contacts": []
    }
  ],
  "alert_contacts": [
    {
      "type": 2,
      "value": "team@neurocred.io",
      "friendly_name": "Team Email"
    }
  ]
}

