# QIE Testnet RPC URL
QIE_TESTNET_RPC_URL=https://testnet.qie.digital
QIE_RPC_URL=https://testnet.qie.digital

# Backend signer private key (must have SCORE_UPDATER_ROLE)
BACKEND_PK=0xYOUR_BACKEND_SIGNER_PRIVATE_KEY
BACKEND_PRIVATE_KEY=0xYOUR_BACKEND_SIGNER_PRIVATE_KEY

# CreditPassportNFT contract address (after deployment)
CREDIT_PASSPORT_ADDRESS=0xYourContractAddress
CREDIT_PASSPORT_NFT_ADDRESS=0xYourContractAddress

# Staking contract address (after deployment)
STAKING_ADDRESS=0xYourStakingContractAddress

# NCRD token address (create via QIEDex or deploy)
NCRD_TOKEN_ADDRESS=0xYourNCRDTokenAddress

# DemoLender contract address (after deployment)
DEMO_LENDER_ADDRESS=0xYourDemoLenderAddress

# QIE Oracle address (USD price oracle)
QIE_ORACLE_USD_ADDR=0xYourOracleAddress

# QIE Explorer URL prefix for transaction links
QIE_EXPLORER_TX_URL_PREFIX=https://testnet.qie.digital/tx
QIE_EXPLORER_URL=https://testnet.qie.digital

# Server port
PORT=8000

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,https://your-frontend.vercel.app
